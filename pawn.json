{
    "user": "Shiska",
    "repo": "pawn",
    "entry": "test.pwn",
    "output": "test.amx",
    "dependencies": [
        "sampctl/samp-stdlib"
    ],
    "dev_dependencies": [
        "pawn-lang/YSI-Includes@5.x",
        "Zeex/samp-plugin-crashdetect"
    ],
    "builds": [
        {
            "name": "amx",
            "args": [
                "-;+", "-(+", "-d3"
            ]
        },
        {
            "name": "compat"
        },
        {
            "name": "lst",
            "args": [
                "-l"
            ]
        },
        {
            "name": "asm",
            "args": [
                "-a"
            ]
        },
        {
            "name": "report",
            "args": [
                "-r"
            ]
        }
    ],
    "runtime": {
        "mode": "y_testing"
    }
}